orbFarm.MouseButton1Click:Connect(function()
	toggle = not toggle
	if toggle == true then
		while toggle == true do
			local pos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
			for i, v in pairs(game:GetService("Workspace").Gems:GetChildren()) do
				if v.Name == 'Gem' then
					v.Position = pos
				end
			end
			wait()
		end
	end
end)

